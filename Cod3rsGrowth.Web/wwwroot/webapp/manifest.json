{
    "_version": "1.58.0",
    "sap.app": {
        "id": "coders-growth",
        "type": "application",
        "i18n": "i18n/i18n.properties",
        "applicationVersion": { "version": "1.0.0" }
    },
	"sap.ui": {
		"technology": "UI5",
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
  	},
	"sap.ui5": {
		"rootView": {
            "id": "app",
            "type": "XML",
            "async": true,
			"viewName": "coders-growth.view.App"
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"type": "View",
				"viewType": "XML",
				"path": "coders-growth.view",
				"controlId": "app",
				"controlAggregation": "pages",
				"transition": "slide",
				"bypassed": {
					"target": "notFound"
				},
				"async": true
			},
			"routes": [{
				"pattern": "",
				"name": "appHome",
				"target": "home"
			}],
			"targets": {
				"home": {
					"id": "home",
					"name": "Home",
					"level" : 1
				},
				"notFound": {
					"id": "notFound",
					"name": "NotFound",
					"transition": "show"
				}
			}
		}
	}
}